{
  "name": "expo-boilerplate",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "node scripts/profile-selector.js start --dev-client",
    "start:dev": "KEYSFILE=keys.development.json expo start --dev-client",
    "start:staging": "KEYSFILE=keys.staging.json expo start --dev-client",
    "start:production": "KEYSFILE=keys.production.json expo start --dev-client",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "node scripts/profile-selector.js android",
    "android:dev": "KEYSFILE=keys.development.json expo run:android",
    "android:staging": "KEYSFILE=keys.staging.json expo run:android",
    "android:production": "KEYSFILE=keys.production.json expo run:android",
    "ios": "node scripts/profile-selector.js ios",
    "ios:dev": "KEYSFILE=keys.development.json expo run:ios",
    "ios:staging": "KEYSFILE=keys.staging.json expo run:ios",
    "ios:production": "KEYSFILE=keys.production.json expo run:ios",
    "web": "expo start --web",
    "prepare": "husky",
    "lint": "expo lint",
    "format": "eslint --fix . && prettier --write . --ignore-path .gitignore",
    "env": "node scripts/env-manager.js",
    "env:encrypt": "node scripts/env-manager.js encrypt",
    "env:list": "node scripts/env-manager.js list",
    "env:status": "node scripts/env-manager.js status",
    "env:setup": "cp keys.example.json keys.development.json",
    "env:verify": "node scripts/verify-env.js",
    "prebuild": "node scripts/profile-selector.js prebuild --clean",
    "prebuild:clean": "expo prebuild --clean",
    "prebuild:dev": "KEYSFILE=keys.development.json expo prebuild --clean",
    "prebuild:staging": "KEYSFILE=keys.staging.json expo prebuild --clean",
    "prebuild:production": "KEYSFILE=keys.production.json expo prebuild --clean"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yaml}": [
      "prettier --write"
    ],
    "**/*.test.{js,jsx,ts,tsx}": [
      "jest --bail --findRelatedTests"
    ],
    "**/*.spec.{js,jsx,ts,tsx}": [
      "jest --bail --findRelatedTests"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.12",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-localization": "^17.0.7",
    "expo-router": "~6.0.10",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "i18next": "^25.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.0.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keys": "^0.7.13",
    "react-native-mmkv": "^3.3.3",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.12",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
